<%= label_tag 'report_options[project_managers]', l(:"cpm.label_select_project_managers") %>
<% if @report.present? and @report[:options].present? %>
	<%= select_tag 'report_options[project_managers]', options_for_select(@report_options, @report[:options][:project_managers]), {:multiple => true, :size => 10} %>
<% else %>
	<%= select_tag 'report_options[project_managers]', options_for_select(@report_options), {:multiple => true, :size => 10} %>
<% end %>